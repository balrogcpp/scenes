import PbrForest from pbr-forest.material
import PbrForest_Shadowed from pbr-forest.material
import Pbr/Unreal from pbr-ue.material
import PbrBasic from pbr-basic.material

material GrassCustom : PbrForest_Shadowed
{
    technique
    {
      pass
      {
        cull_hardware none
        cull_software none
        alpha_rejection greater_equal 128

        texture_unit Albedo
        {
            texture grassWalpha.dds
        }
      }
	}
}

material 3D-Diggers/fir01 : PbrBasic
{
    receive_shadows off
    technique
    {
      pass
      {
        alpha_rejection greater_equal 128
        texture_unit Albedo
        {
            texture fir01.dds
        }
      }
	}
}

material 3D-Diggers/fir02 : Pbr/Unreal
{
    technique
    {
      pass
      {
        texture_unit Albedo
        {
            texture white-spruce-tree-bark-albedo.dds
        }
        texture_unit Normal
        {
            texture white-spruce-tree-bark-normal-dx.dds
        }
        texture_unit Metalness
        {
            texture white-spruce-tree-bark-metalness.dds
        }
        texture_unit Roughness
        {
            texture white-spruce-tree-bark-roughness.dds
        }
      }
	}
}

material 3D-Diggers/farn01 : PbrForest_Shadowed
{
    receive_shadows on
    technique
    {
      pass
      {
        alpha_rejection greater_equal 128

        texture_unit Albedo
        {
            texture farn01.dds
        }
	}
	}
}

material 3D-Diggers/farn02 : PbrForest
{
    technique
    {
      pass
      {
        texture_unit Albedo
        {
            tex_address_mode clamp
            texture farn02.dds
        }
	}
	}
}

